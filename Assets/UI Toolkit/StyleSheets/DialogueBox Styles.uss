.character-sprite-box {
    width: 350px;
    height: 432px;
    margin-left: 16%;
    margin-right: 16%;
    background-color: rgba(0, 0, 0, 0.36);
    overflow: hidden;
}

.character-sprite {
    width: 100%;
    height: 100%;
    padding-left: 5%;
    padding-right: 5%;
    padding-top: 5%;
    padding-bottom: 0;
    background-image: none;
    -unity-background-scale-mode: scale-to-fit;
}
